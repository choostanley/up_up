<%= form_for sessions_path do |f| %>

  <p>
  	<%= f.label "Name" %> <br>
  	<%= f.text_field :name %>
  </p>
  <p>
    <%= f.label "E-mail" %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label "Password" %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit 'Sign In' %>
  </p>
<% end %>